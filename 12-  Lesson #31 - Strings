#include <iostream>
#include <string>
using namespace std;


int main()
{
	string MyString = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
	cout << "The length of the MyString is : " << MyString.length() << endl;
	
	cout << MyString[2] << endl; //will give C

	string S1 = "10", S2 = "20";

	string S3 = S1 + S2;

	cout << S3 << endl; //will give 1020

	int Sum = stoi(S1) + stoi(S2);

	cout << Sum << endl; //will give 30

	return 0;
}
