#include <iostream>
#include <string>

using namespace std;


int main()
{
    cout << "\n For Loop \n";
    for (int x = 1; x <= 5; x++)
    {
        cout << x << endl;

    }

    cout << "\n While Loop \n";
    int i = 1;
    while (i <= 5)
    {
        cout << i << endl;
        i++;
    
    }
    return 0;
}
