#include <iostream>
#include <string>
using namespace std;


int main()
{
    int n;
    int sum = 0;
    cout << "please enter n : " << endl;
    cin >> n;
    int i = 1;
    while (i <= n)
    {
        
        if (i % 2 != 0)
        {
            sum += i;
        }
        i++;
    }
 
    cout << sum;

    return 0;
}
