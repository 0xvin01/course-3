#include <iostream>
using namespace std;



int main()
{
	int month;
	cout << " please enter month :" << endl;
	cin >> month;

	if (month == 1)
	{
		cout << "jan" << endl;
	}
	else if (month == 2)
	{
		cout << "feb" << endl;
	}
	else if (month == 3)
	{
		cout << "march" << endl;
	}
	else if (month == 4)
	{
		cout << "apr" << endl;
	}
	else if (month == 5)
	{
		cout << "may" << endl;
	}
	else if (month == 6)
	{
		cout << "june" << endl;
	}
	else if (month == 7)
	{
		cout << "july" << endl;
	}
	else if (month == 8)
	{
		cout << "aug" << endl;
	}
	else if (month == 9)
	{
		cout << "sep" << endl;
	}
	else if (month == 10)
	{
		cout << "oct" << endl;
	}
	else if (month == 11)
	{
		cout << "nov" << endl;
	}
	else if (month == 12)
	{
		cout << "dec" << endl;
	}
	else
	{
		cout << "wrong month" << endl;
	}
	return 0;
}
