#include <iostream>
using namespace std;

int main()
{

	int A, B;

	cout << "Please enter A : " << endl;
	cin >> A;

	cout << "Please enter B : " << endl;
	cin >> B;

	cout << A << " = " << B << " is " << (A == B) << endl;
	cout << A << " ! " << B << " is " << (A != B) << endl;
	cout << A << " > " << B << " is " << (A > B) << endl;
	cout << A << " < " << B << " is " << (A < B) << endl;
	cout << A << " >= " << B << " is " << (A >= B) << endl;
	cout << A << " <= " << B << " is " << (A <= B) << endl;



	return 0;
}
